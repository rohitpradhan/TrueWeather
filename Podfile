# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'
use_frameworks!

def sharedPods
  pod 'SDWebImage', '~> 4.0'

end

target 'TrueWeather' do
    sharedPods
end

target 'TrueWeatherTests' do
    inherit! :search_paths
    sharedPods
end

target 'TrueWeatherUITests' do
    inherit! :search_paths
    sharedPods
end


